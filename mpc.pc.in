prefix=@prefix@
exec_prefix=@exec_prefix@
includedir=@includedir@
libdir=@libdir@

Name: GNU MPC
Description: A complex floating-point library with exact rounding
URL: https://www.multiprecision.org/mpc/
Version: @VERSION@
Requires: mpfr >= @REQUIRES_MPFR_VERSION@
Requires.private: gmp >= @REQUIRES_GMP_VERSION@
Cflags: -I${includedir}
Libs: -L${libdir} -lmpc
